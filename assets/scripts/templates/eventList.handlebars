<div class="list-header">

        <h3>Todo List<a id="createEventButton" title="Create a list" role="button">       <i class="fa fa-plus-square" aria-hidden="true"></i></a></h3>


<div class="create">
  <form id="create-event">

    <!-- <div class="input-group"> -->
      <!-- <label for="title">Todo Description:</label> -->
      <input type="text" class="form-control" name="event[title]" placeholder="What next?" required>
      <!-- </div> -->
      <!-- <input type="hidden" class="form-control" name="event[user_id]" value="1">
      <input type="hidden" class="form-control" name="event[event_id]" value="1"> -->
      <button type="submit"><i class="fa fa-check-circle-o" aria-hidden="true"></i></button>
       <a class="cancel-new-list" href=""><i class="fa fa-times-circle-o" aria-hidden="true"></i></a>
    <!-- <input type="submit" name="create-event" class="btn btn-info" value="enter"> -->
  </form>
</div>
</div>

<div class="list">
        <!-- <button type="Submit" id='eventsShow' class="btn btn-info">View list >></button> -->

          <ul>
            {{#if events}}
              {{#each events}}
              <hr>
               <h3><li data-id="{{id}}" {{#if complete}}class="complete"{{/if}}><input type="checkbox" {{#if complete}}checked{{/if}} class="complete">          <a href="" class="view-list" data-id={{id}}>{{title}}</a>
               <a href="" class="deleteEvent" data-id={{id}}><i class="fa fa-trash-o" aria-hidden="true"></i></a></li></h3>
              {{/each}}
            {{else}}
              <p>Start your list.</p>
            {{/if}}
          </ul>
    </div>
